/* address.wxss */
/* 没数据时图像不可见 */
.nullImage{
  width: 5rem;
  margin-top: 6.75rem;
  /* margin-left: 7rem; */
  height: 3rem;
  display: inline-block;
}
.holeView{
  text-align: center;
}
.nullAddress{
  font-size: 1rem;
}

.content{
  width: 100%;
  background: #fff;
  border-radius: 10rpx;
}
.session {
  display:inline-block;
  padding: 10px;
}
.sessoin-tit{
  font-size: 26rpx;
  color: #666666;
  vertical-align: middle;
  padding:0 15rpx;
  width: 140rpx;
}
.session-val{
  font-size: 14px;
  color: #666666;
  display: inline-block;
  border-bottom: 0px solid #000;
  margin-top: 32rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow:ellipsis;
}
.myBtn{
   background-color:  #2D2D2D ;
   color: #fff;
   font-size: 14px;
   height: 50px;
   line-height: 50px;
   margin-top: 100rpx;
}
.touch-item {
 font-size: 14px;
 display: flex;
 justify-content: space-between;
 border-bottom:1px solid #ccc;
 width: 100%;
 overflow: hidden;
 transition:all 0.4s;
 /* flex-direction: column; */
}
