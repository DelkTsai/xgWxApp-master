.del {
 background-color: #2D2D2D;
 width: 90px;
 display: inline-block;
 color: #fff;
 transform: scaleX(90px);
 transition: width 0.4s;
}
.touch-move-active.touch-item{
  transform: translateX(-90px);
}
.touch-move-active.del {
 width: 90px;
}
/* 选择框距离顶部的距离 */
.topDistance{
  margin-top: 1.8em;
  margin-right: 1em;
}
/* 右侧编辑图片的大小 */
.editImage{
  width: 20px;
  height: 20px;
  position:relative;
  top:50%;
  transform:translateY(-50%);

}
.rightImage{
  margin-left: 3em;
}
/* checkbox变圆 */
.checkR{
  border-radius: 1em;
}
/* 新增按钮保持在底部 */
.footer{
  position: absolute;
  bottom: 0px;
  width: 100%;
}
.leftWidth{
  width:50%
}
