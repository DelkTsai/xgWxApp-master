/* myCouponList.wxss */
/* 优惠券列表 */
.couponViewList{
  width: 100%;
  height: 105px;
  text-align: center;
  position: relative;
  margin-left: 0.8%;
  margin-top: 1em;
}
/* 优惠券底图 */
.couponImage{
  width: 90%;
   height: 95px; 
}
/* 优惠券标题 */
.couponTitle{
  position: absolute;
  left: 50px;
  top: 27px;
  color: #FFFFFF;
  font-size: 17px;
}
/* 优惠券有效期 */
.couponDate{
  position: absolute;
  font-size: 12px;
  color: #D0D0D0;
  left: 50px;
  top: 50px;
}
/* 当没有列表的时候 */
.noList{
  text-align: center;
  margin-top: 140px;
}
.noImage{
  width: 96px;
  height: 43px;
}
.noText{
  color: #000000;
  font-size: 12px;
}