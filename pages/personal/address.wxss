 transform: translateX(-90px);
}
/* 选择框距离顶部的距离 */
.topDistance{
  margin-top: 1.8em;
  margin-right: 1em;
}
/* 右侧编辑图片的大小 */
.editImage{
  width: 13px;
  height: 16px;
}
.rightImage{
  margin-left: 3em;
}
/* checkbox变圆 */
.checkR{
  border-radius: 1em;
}
/* 新增按钮保持在底部 */
.footer{
  position: absolute;
  bottom: 0px;
  width: 100%;
}
.shadowBorder{
  box-shadow: 0px 2px 5px #DDDDDD;

