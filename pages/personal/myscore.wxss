/* myscore.wxss */
.scoreShow{
  width: 100%;
  height: 130px;
  position: relative;
  box-shadow:0px 2px 5px #DDDDDD;
}
.scoreText{
  font-size: 14px;
  color: #7D7D7D;
  position: absolute;
  left: 31px;
  top: 27px;
}
.scoreValue{
  font-size: 44px;
  color: #000000;
  position: absolute;
  left: 31px;
  top: 45px;
}
/* 下方按钮样式 */
.buttonView{
  position: absolute;
  bottom: 20px;
  text-align: center;
  width: 100%;
}
.changeCoupon{
  width: 80%;
  background-color: #000000;
  color: #FFFFFF;
  font-size: 14px;
  border-radius: 10px;
  margin-bottom: 10px;
}
.changePiece{
  width: 80%;
  background-color: #FFFFFF;
  color: #000000;
  font-size: 14px;
  border-radius: 10px;
  border: 1px #000000 solid;
}