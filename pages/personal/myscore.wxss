/* myscore.wxss */
page{
  background: #F4F4F4;
}
.scoreShow{
  width: 100%;
  height: 130px;
  position: relative;
  box-shadow:0px 2px 5px #DDDDDD;
  background: white;
}
.scoreText{
  font-size: 14px;
  color: #7D7D7D;
  position: absolute;
  left: 31px;
  top: 27px;

}
.scoreValue{
  font-size: 44px;
  color: #000000;
  position: absolute;
  left: 31px;
  top: 45px;
    font-weight: lighter;
  font-family: system;
}
/* 下方按钮样式 */
.buttonView{
  position: absolute;
  bottom: 20px;
  text-align: center;
  width: 100%;
}
.changeCoupon{
  width: 80%;
  background-color: #2d2d2d;
  color: #FFFFFF;
  font-size: 14px;
  border-radius: 10px;
  margin-bottom: 10px;
  height: 42px;
  line-height: 42px;
}
.changePiece{
  width: 80%;
  background-color: #FFFFFF;
  color: #000000;
  font-size: 14px;
  border-radius: 10px;
  border: 1px #000000 solid;
}