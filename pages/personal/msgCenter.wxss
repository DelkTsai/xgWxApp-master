/* 分割线 */
.fenge{
  width: 100%;
  height: 5px;
  background-color: #F7F7F7;
}

/* 每个item的样式 */
.itemView{
  width: 100%;
  height: 130px;
  position: relative;
  margin-top: 3vh;
  margin-bottom: 5vh;
  /* box-shadow: 0px 5px 2px 5px #888888; */
}
/* 标题样式 */
.titleView{
  font-size: 16px;
  color: #aaaaaa;
  border-bottom:2px solid #7d7d7d;
  padding-left:21px; 
}
/* 内容样式 */
.contentView{
  position: absolute;
  left: 20px;
  font-size: 14px;
  color: #000;
}
/* 创建时间和立即查看 */
.createAndLook{
  position: relative;
  width: 100%;
  height: 20px;
  bottom: 7px;
  left: 0px;
}
.createView{
  position: absolute;
  left: 21px;
  font-size: 12px;
  color: #7d7d7d;
  top: 90px;
}
.lookView{
  position: absolute;
  right: 24px;
  top: 90px;
  color: #7d7d7d;
  font-size: 13px;
}
