/* buyCarList.wxss */
.content{
  width: 100%;
  box-sizing: border-box;
  /* margin: -21rpx auto; */
  background: #fff;
  min-height: 400rpx;
  /* padding: 50rpx 25rpx; */
  position: relative;
     box-shadow: 0 0 20rpx 4rpx  #becee7; 
  border-radius: 10rpx; 
  margin-top: 1em; 

}
.session {
  display:flex;
  height: 120rpx;
  line-height: 120rpx;
}
.sessoin-tit{
  font-size: 26rpx;
  color: #666666;
  vertical-align: middle;
  padding:0 15rpx;
  width: 140rpx;
}
.session-val{
  font-size: 26rpx;
  color: #666666;
  display: block;
  line-height: 1;
  width: 340rpx;
  border-bottom: 1px solid #ddd;
  margin-top: 32rpx;
}
.myBtn{
   background-color:  #2D2D2D ;
   color: #fff;
   font-size: 26rpx;
   height: 88rpx;
   line-height: 88rpx;
   margin-top: 100rpx;
}
/* 商品图片 */
.goodsImage{
  width:8.92rem;
  height:8.92rem;
  margin-top: 0.92rem;
  margin-right: 0.75rem;
}
/* 属性图片 */
.attrImage{
  width: 25px;
  height: 25px;
  margin-right: 1em;
}
/* checkBox的样式 */
.checkbx{
  border-radius: 5px;
  margin-left: 0.75em;
  margin-top:4.75rem;
}
/* 减号样式 */
.mines{
  width: 1.67rem;
  height: 1.67rem;
  margin-right: 1em;
}
/* 加号样式 */
.plus{
  width: 1.67rem;
  height: 1.67rem;
  margin-left: 1em;
}
/* 码数和份数的通用样式 */
.tong{
  margin-top: -1em;
}
/* 码数的样式 */
.mashu{
display: inline;
}



/* 弹窗样式 */
.btn { 
  width: 80%;  
  padding: 20rpx 0;  
  border-radius: 10rpx;  
  text-align: center;  
  margin: 40rpx 10%;  
  background: #000;  
  color: #fff;  
} 
   
/*mask*/ 
.drawer_screen { 
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #6F6F6F;  
  opacity: 0.95;  
  overflow: hidden;  
} 
   
/*content*/ 
.drawer_box { 
  width: 100%;  
  overflow: hidden;  
  position: fixed;  
  top: 30%;  
  left: 0;  
  z-index: 1001;  
  background: #FAFAFA;  
  /* margin: -150px 50rpx 0 50rpx;   */
  border-radius: 3px;  
  background-color: #000000;
} 
   
.drawer_title{ 
  padding:15px;  
  font: 20px "microsoft yahei";  
  text-align: center; 
  color: #FFFFFF; 
} 
.drawer_content { 
  /* height: 210px;   */
  overflow-y: scroll; /*超出父盒子高度可滚动*/ 
} 
   
.btn_ok{ 
  padding: 10px;  
  font: 20px "microsoft yahei";  
  text-align: center;  
  border-top: 1px solid #E8E8EA;  
  color: #3CC51F;  
  background-color: #FFFFFF;
} 
   
.top{ 
    padding-top:8px;  
} 
.bottom { 
    padding-bottom:8px;  
} 
.title { 
    height: 30px;  
    line-height: 30px;  
    width: 160rpx;  
    text-align: center;  
    display: inline-block;  
    font: 300 28rpx/30px "microsoft yahei";  
} 
   
.input_base { 
    border: 2rpx solid #ccc;  
    padding-left: 10rpx;  
    margin-right: 50rpx;  
} 
.input_h30{ 
    height: 30px;  
    line-height: 30px;  
} 
.input_h60{ 
    height: 60px;  
} 
.input_view{ 
    font: 12px "microsoft yahei";  
    background: #fff;  
    color:#000;  
    line-height: 30px;  
} 
   
input { 
    font: 12px "microsoft yahei";  
    background: #fff;  
    color:#000 ;  
} 
radio{ 
    margin-right: 20px;  
} 
.grid { display: -webkit-box; display: box; } 
.col-0 {-webkit-box-flex:0;box-flex:0;} 
.col-1 {-webkit-box-flex:1;box-flex:1;} 
.fl { float: left;} 
.fr { float: right;} 
/* 颜色样式 */
.co{
  width: 20px;
  height: 20px;
  border-radius: 10px;
}
/* 商品属性 */
.attrClass{
  height: 70px;
}
/* 计算窗口 */
.footerSum{
  position: fixed;
  width: 100%;
  height: 50px;
  left: 0px;
  bottom: 0px;
  background-color: white;
}
.footerTotal{
  display: inline-block;
  float: left;
  width: 33%;
}
.selectButtom{
  margin-top: 0.5em;
}
.sumButton{
  background-color: #000;
  font-size: 14px;
  color: #fff;
  text-align: center;
  height: 50px;
}
.sumBut{
  margin-top: 1em;
  display: block;
}
.quanText{
  color: #7d7d7d;
  font-size: 14px;
  margin-top: 1em;
}
.hijitext{
  display: block;
  margin-top: 1em;
}

